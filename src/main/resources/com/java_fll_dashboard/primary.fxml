<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Slider?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.java_fll_dashboard.PrimaryController">
   <children>
      <Label text="Primary View" />
      
      <Button fx:id="PrimaryButton" text="Switch to Secondary View" onAction="#switchToSecondary"/>
      <Slider fx:id="mySlider" min="0" max="100" value="50"/>
      <Button fx:id="EAsports" text="Print EA sports" onAction="#testTrigger"/>
   </children>
   <padding>
      <Insets bottom="22.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
